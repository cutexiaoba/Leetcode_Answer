/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
/* You must change the actual object; instead of the pointer. */
class Solution {
public:
    void deleteNode(ListNode* node) {
        *node = *node -> next;
    }
};
